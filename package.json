        "hardhat": "^2.0.0",
        "hardhat-deploy": "^0.9.0",
        "mocha": "^9.1.3",
        "prettier": "2.5.1",
        "ts-node": "^10.4.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "tslint-plugin-prettier": "^2.0.1",
